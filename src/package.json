{
  "name": "hww.leavetracker",
  "version": "1.0.0",
  "description": "Leave tracker for Hangar worldwide",
  "main": "./build/main.js",
  "scripts": {
    "tsc": "tsc",
    "test": "echo \"Error: no test specified\" && exit 1",
    "dev": "NODE_ENV=dev ts-node-dev --respawn --transpileOnly ./app.ts",
    "play": "NODE_ENV=play ts-node-dev --respawn --transpileOnly ./index.ts",
    "prod": "npm run clean && tsc && npm run copy:assets && NODE_ENV=prod node ./deploy/app.js",
    "clean": "rimraf deploy",
    "copy:assets": "cp -r views deploy"
  },
  "author": "Sandeep Baynes",
  "license": "ISC",
  "dependencies": {
    "@types/config": "0.0.34",
    "@types/express": "^4.17.1",
    "config": "^3.2.2",
    "express": "^4.17.1",
    "ts-node-dev": "^1.0.0-pre.42",
    "typescript": "^3.6.3"
  }
}
